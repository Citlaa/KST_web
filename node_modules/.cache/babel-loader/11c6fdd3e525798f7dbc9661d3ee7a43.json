     filtro: {\r\n            activo: 1,\r\n          },\r\n        };\r\n\r\n        const response = await axios.post(\r\n          routeAPI + \"catalogo/tiposDeGrupo\",\r\n          filtros\r\n        );\r\n\r\n      if (!response.data.hayError){\r\n        if (response.data.response.length > 0){\r\n          response.data.response.forEach((element) => {\r\n          this.items.push({\r\n            TipoGrupoId: element[\"006TipoDeGrupoId\"],\r\n            Nombre: element[\"006Nombre\"],\r\n            Activo: element[\"006Activo\"],\r\n          });\r\n        });\r\n        }\r\n      }else{\r\n        console.log(response);\r\n        this.$alert(\r\n          \"No se pudo obtenera informaci√≥n, favor de volverlo a intentar.\"\r\n        );\r\n      }\r\n      this.isLoading = false;\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n    seleccionar: function() {\r\n      this.$emit(this.$props.funcion, this.valor);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/Catalogos/Selects"}]}