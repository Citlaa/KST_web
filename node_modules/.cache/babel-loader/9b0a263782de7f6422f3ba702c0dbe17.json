ADA,oCACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AAEA,gBAAA,OAJA,GAIA;AACA,kBAAA,MAAA,EAAA;AACA,oBAAA,MAAA,EAAA;AADA;AADA,iBAJA;AAAA;AAAA,uBAUA,KAAA,CAAA,IAAA,CACA,QAAA,GAAA,4CADA,EAEA,OAFA,CAVA;;AAAA;AAUA,gBAAA,QAVA;;AAcA,oBAAA,CAAA,QAAA,CAAA,IAAA,CAAA,QAAA,EAAA;AACA,sBAAA,QAAA,CAAA,IAAA,CAAA,QAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,oBAAA,QAAA,CAAA,IAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,sBAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,wBAAA,oBAAA,EAAA,OAAA,CAAA,yBAAA,CADA;AAEA,wBAAA,WAAA,EAAA,OAAA,CAAA,gBAAA,CAFA;AAGA,wBAAA,YAAA,EAAA,OAAA,CAAA,iBAAA,CAHA;AAIA,wBAAA,MAAA,EAAA,OAAA,CAAA,WAAA;AAJA,uBAAA;AAMA,qBAPA;AAQA;AACA,iBAXA,MAWA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,QAAA;;AACA,kBAAA,KAAA,CAAA,MAAA,CACA,+DADA;AAGA;;AACA,gBAAA,KAAA,CAAA,SAAA,GAAA,KAAA;AACA,oBAAA,KAAA,CAAA,MAAA,CAAA,MAAA,GAAA,CAAA,EACA,KAAA,CAAA,iBAAA;AAjCA;AAAA;;AAAA;AAAA;AAAA;AAoCA,gBAAA,OAAA,CAAA,GAAA;;AApCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsCA,KAvCA;AAwCA,IAAA,WAAA,EAAA,uBAAA;AACA,WAAA,KAAA,GAAA,CAAA,GAAA,KAAA,KAAA,CAAA,KAAA,MAAA,CAAA,OAAA,EAAA,KAAA,KAAA,CAAA,GAAA,KAAA,KAAA,CAAA,KAAA,MAAA,CAAA,OAAA,EAAA,IAAA,CAAA;AACA,KA1CA;AA2CA,IAAA,iBAAA,EAAA,6BAAA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,GAAA,WAAA,EAAA;AACA,UAAA,OAAA,GAAA,KAAA,KAAA,CAAA,MAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,WAAA,IAAA,IAAA,IAAA,CAAA,CAAA,YAAA,IAAA,IAAA;AAAA,OAAA,CAAA;AACA,WAAA,KAAA,GAAA,OAAA,CAAA,OAAA,CAAA,MAAA,GAAA,CAAA,CAAA,CAAA,oBAAA;AAEA,WAAA,WAAA;AACA;AAjDA;AAxBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"col-12\">\r\n      <label v-if=\"titulo\" class=\"activo_label\">{{ label }}</label>\r\n      <select\r\n        class=\"form-control\"\r\n        v-model=\"valor\"\r\n        @change=\"seleccionar()\"\r\n        :disabled=\"disabled\"\r\n      >\r\n        <option value=\"-1\">Seleccionar Ciclo Escolar</option>\r\n        <option\r\n          v-for=\"(element, index) in items\"\r\n          :key=\"index\"\r\n          :value=\"element['TipoDeCicloEscolarId']\"\r\n          >{{ element[\"AñoDeInicio\"] }} - {{ element[\"AñoDeTermino\"] }}</option\r\n        >\r\n      </select>\r\n    </div>\r\n    <cargando v-if=\"isLoading\"></cargando>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport routeAPI from \"@/js/api\";\r\n\r\nexport default {\r\n  emits: ['seleccionarCicloEscolar'],\r\n  data() {\r\n    return {\r\n      valor: \"-1\",\r\n      isLoading: false,\r\n      items: [],\r\n    };\r\n  },\r\n  props: {\r\n    label: String,\r\n    funcion: String,\r\n    tipoDeCicloEscolarId: Number,\r\n    titulo: { type: Boolean, required: true, default: true },\r\n    disabled: Boolean,\r\n    actual: { type: Boolean, default: false }\r\n  },\r\n  created() {\r\n    this.getTiposDeCicloEscolar();\r\n\r\n    if (this.$props.tipoDeCicloEscolarId > 0)\r\n      this.valor = this.$props.tipoDeCicloEscolarId;    \r\n  },\r\n  computed: {},\r\n  methods: {\r\n    async getTiposDeCicloEscolar() {\r\n      try {\r\n        this.isLoading = true;\r\n\r\n        const filtros = {\r\n          filtro: {\r\n            activo: 1,\r\n          },\r\n        };\r\n\r\n        const response = await axios.post(\r\n          routeAPI + \"administracion/tiposDeCicloEscolarCatalogo\",\r\n          filtros\r\n        );\r\n        if (!response.data.hayError){\r\n          if (response.data.response.length > 0){\r\n          response.data.response.forEach((element) => {\r\n          this.items.push({\r\n            TipoDeCicloEscolarId: element[\"002TipoDeCicloEscolarId\"],\r\n            AñoDeInicio: element[\"002AñoDeInicio\"],\r\n            AñoDeTermino: element[\"002AñoDeTermino\"],\r\n            Activo: element[\"002Activo\"],\r\n          });\r\n        });\r\n        }\r\n      }else{\r\n        console.log(response);\r\n        this.$alert(\r\n          \"No se pudo obtener información, favor de volverlo a intentar.\"\r\n        ); \r\n      }\r\n        this.isLoading = false;\r\n        if (this.$props.actual > 0)\r\n          this.seleccionarActual();\r\n\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n    seleccionar: function() {\r\n      this.valor > 0 ? this.$emit(this.$props.funcion, this.valor) : this.$emit(this.$props.funcion, null);\r\n    },\r\n    seleccionarActual: function(){\r\n      const year = new Date().getFullYear();      \r\n      var options = this.items.filter(i => i.AñoDeInicio == year || i.AñoDeTermino == year);\r\n      this.valor = options[options.length - 1].TipoDeCicloEscolarId;     \r\n      \r\n      this.seleccionar();\r\n    }\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/Catalogos/Selects"}]}