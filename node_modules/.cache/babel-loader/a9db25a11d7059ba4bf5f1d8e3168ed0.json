AAA,CAAA,mBAAA;AACA,oBAAA,KAAA,CAAA,mBAAA,CAAA,YAAA,IAAA,EAAA,EACA,OAAA,CAAA,MAAA,CAAA,KAAA,GAAA,MAAA,CAAA,KAAA,CAAA,mBAAA,CAAA,YAAA,CAAA;AACA,oBAAA,KAAA,CAAA,mBAAA,CAAA,gBAAA,IAAA,EAAA,EACA,OAAA,CAAA,MAAA,CAAA,SAAA,GAAA,MAAA,CACA,KAAA,CAAA,mBAAA,CAAA,gBADA,CAAA;AAGA,oBAAA,KAAA,CAAA,mBAAA,CAAA,cAAA,IAAA,EAAA,EACA,OAAA,CAAA,MAAA,CAAA,OAAA,GAAA,MAAA,CACA,KAAA,CAAA,mBAAA,CAAA,cADA,CAAA;AAGA,oBAAA,KAAA,CAAA,mBAAA,CAAA,YAAA,IAAA,EAAA,EACA,OAAA,CAAA,MAAA,CAAA,KAAA,GAAA,MAAA,CAAA,KAAA,CAAA,mBAAA,CAAA,YAAA,CAAA;AAvBA;AAAA,uBAyBA,KAAA,CAAA,IAAA,CACA,QAAA,GAAA,0CADA,EAEA,OAFA,CAzBA;;AAAA;AAyBA,gBAAA,QAzBA;;AA8BA,oBAAA,CAAA,QAAA,CAAA,IAAA,CAAA,QAAA,EAAA;AACA,sBAAA,QAAA,CAAA,IAAA,CAAA,QAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,oBAAA,QAAA,CAAA,IAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,sBAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,wBAAA,mBAAA,EAAA,OAAA,CAAA,wBAAA,CADA;AAEA,wBAAA,aAAA,EAAA,OAAA,CAAA,oBAAA,CAFA;AAGA,wBAAA,WAAA,EAAA,OAAA,CAAA,gBAAA,CAHA;AAIA,wBAAA,SAAA,EAAA,OAAA,CAAA,gBAAA,CAJA;AAKA,wBAAA,MAAA,EAAA,OAAA,CAAA,WAAA;AALA,uBAAA;AAOA,qBARA;AASA;AACA,iBAZA,MAaA,KAAA,CAAA,MAAA,CACA,+DADA;;AAIA,gBAAA,KAAA,CAAA,SAAA,GAAA,KAAA;AA/CA;AAAA;;AAAA;AAAA;AAAA;AAiDA,gBAAA,OAAA,CAAA,GAAA;;AAjDA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmDA,KApDA;AAqDA,IAAA,WAAA,EAAA,uBAAA;AACA,WAAA,KAAA,GAAA,CAAA,GACA,KAAA,KAAA,CAAA,KAAA,MAAA,CAAA,OAAA,EAAA,KAAA,KAAA,CADA,GAEA,KAAA,KAAA,CAAA,KAAA,MAAA,CAAA,OAAA,EAAA,IAAA,CAFA;AAGA;AAzDA;AAtBA,CAAA","sourcesContent":["<template>\n  <section>\n    <div class=\"col-12\">\n      <label v-if=\"titulo\" class=\"activo_label\">{{ label }}</label>\n      <select\n        class=\"form-control\"\n        v-model=\"valor\"\n        @change=\"seleccionar()\"\n        :disabled=\"disabled\"\n      >\n        <option value=\"-1\">Seleccionar Grupo</option>\n        <option\n          v-for=\"(element, index) in items\"\n          :key=\"index\"\n          :value=\"element['EstructuraDeGrupoId']\"\n          >{{element[\"TipoGrado\"]}} {{element[\"TipoDeGrupo\"]}} - {{element[\"TipoModalidad\"]}}</option\n        >\n      </select>\n    </div>\n  </section>\n</template>\n<script>\nimport axios from \"axios\";\nimport routeAPI from \"@/js/api\";\n\nexport default {\n  data() {\n    return {\n      valor: \"-1\",\n      isLoading: false,\n      items: [],\n    };\n  },\n  props: {\n    label: String,\n    funcion: String,\n    tipoDeGrupoId: Number,\n    titulo: { type: Boolean, required: true, default: true },\n    filtrosEstablecidos: {},    \n    disabled: Boolean,\n  },\n  created() {\n    this.getGrupos();\n\n    if (this.$props.tipoDeGrupoId > 0) this.valor = this.$props.tipoDeGrupoId;\n  },\n  computed: {},\n  methods: {\n    async getGrupos() {\n      try {\n        this.isLoading = true;\n        this.items = [];\n        const filtros = {\n          filtro: {\n            activo: 1,\n          },\n        };\n\n        if (this.filtrosEstablecidos.filtro_cicloEscolar != \"\")\n          filtros.filtro.cicloEscolar = this.filtrosEstablecidos.filtro_cicloEscolar;\n        if (this.filtrosEstablecidos.filtro_nivel != \"\")\n          filtros.filtro.nivel = Number(this.filtrosEstablecidos.filtro_nivel);\n        if (this.filtrosEstablecidos.filtro_modalidad != \"\")\n          filtros.filtro.modalidad = Number(\n            this.filtrosEstablecidos.filtro_modalidad\n          );\n        if (this.filtrosEstablecidos.filtro_periodo != \"\")\n          filtros.filtro.periodo = Number(\n            this.filtrosEstablecidos.filtro_periodo\n          );\n        if (this.filtrosEstablecidos.filtro_grado != \"\")\n          filtros.filtro.grado = Number(this.filtrosEstablecidos.filtro_grado);\n\n        const response = await axios.post(\n          routeAPI + \"administracion/estructurasDeGrupoNombres\",\n          filtros\n        );\n        \n        if (!response.data.hayError) {\n          if (response.data.response.length > 0) {\n            response.data.response.forEach((element) => {\n              this.items.push({\n                EstructuraDeGrupoId: element[\"010EstructuraDeGrupoId\"],\n                TipoModalidad: element[\"004NombreModalidad\"],\n                TipoDeGrupo: element[\"006NombreGrupo\"],\n                TipoGrado: element[\"008NombreGrado\"],\n                Activo: element[\"010Activo\"],\n              });\n            });\n          }\n        } else\n          this.$alert(\n            \"No se pudo obtener información, favor de volverlo a intentar.\"\n          );\n\n        this.isLoading = false;\n      } catch (err) {\n        console.log(err);\n      }\n    },\n    seleccionar: function() {\n      this.valor > 0\n        ? this.$emit(this.$props.funcion, this.valor)\n        : this.$emit(this.$props.funcion, null);\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/components/Catalogos/Selects"}]}