AA,IAAA,CAAA,QAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,oBAAA,QAAA,CAAA,IAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,sBAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,wBAAA,kBAAA,EAAA,OAAA,CAAA,uBAAA,CADA;AAEA,wBAAA,MAAA,EAAA,OAAA,CAAA,WAAA,CAFA;AAGA,wBAAA,MAAA,EAAA,OAAA,CAAA,WAAA;AAHA,uBAAA;AAKA,qBANA;AAOA;AACA,iBAVA,MAUA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,QAAA;;AACA,kBAAA,KAAA,CAAA,MAAA,CACA,+DADA;AAGA;;AACA,gBAAA,KAAA,CAAA,SAAA,GAAA,KAAA;AA9BA;AAAA;;AAAA;AAAA;AAAA;AAgCA,gBAAA,OAAA,CAAA,GAAA;;AAhCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkCA,KAnCA;AAoCA,IAAA,WAAA,EAAA,uBAAA;AACA,WAAA,KAAA,CAAA,KAAA,MAAA,CAAA,OAAA,EAAA,KAAA,KAAA;AACA;AAtCA;AAtBA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"col-12\">\n      <label v-if=\"titulo\" class=\"activo_label\">{{ label }}</label>\n      <select\n        class=\"form-control\"\n        v-model=\"valor\"\n        @change=\"seleccionar()\"\n        :disabled=\"disabled\"\n      >\n        <option value=\"-1\">Seleccionar Especialidad</option>\n        <option\n          v-for=\"(element, index) in items\"\n          :key=\"index\"\n          :value=\"element['TipoEspecialidadId']\"\n          >{{ element[\"Nombre\"] }}</option\n        >\n      </select>\n    </div>\n    <cargando v-if=\"isLoading\"></cargando>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nimport routeAPI from \"@/js/api\";\n\nexport default {\n  data() {\n    return {\n      valor: \"-1\",\n      isLoading: false,\n      items: [],\n    };\n  },\n  props: {\n    label: String,\n    funcion: String,\n    TipoEspecialidadId: Number,\n    titulo: { type: Boolean, required: true, default: true },\n    disabled: Boolean,\n  },\n  created() {\n    this.getEspecialidades();\n\n    if (this.$props.TipoEspecialidadId > 0)\n      this.valor = this.$props.TipoEspecialidadId;\n  },\n  computed: {},\n  methods: {\n    async getEspecialidades() {\n      try {\n        this.isLoading = true;\n        const filtros = {\n          filtro: {\n            activo: 1,\n          },\n        };\n\n        const response = await axios.post(\n          routeAPI + \"catalogo/especialidades\",\n          filtros\n        );\n\n        if (!response.data.hayError){\n          if (response.data.response.length > 0){\n              response.data.response.forEach((element) => {\n              this.items.push({\n                TipoEspecialidadId: element[\"009TipoEspecialidadId\"],\n                Nombre: element[\"009Nombre\"],\n                Activo: element[\"009Activo\"],\n              });\n             });\n          }\n        }else{\n        console.log(response);\n        this.$alert(\n          \"No se pudo obtener informaci√≥n, favor de volverlo a intentar.\"\n        ); \n      }\n      this.isLoading = false;\n      } catch (err) {\n        console.log(err);\n      }\n    },\n    seleccionar: function() {\n      this.$emit(this.$props.funcion, this.valor);\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/components/Catalogos/Selects"}]}