\n          filtros\n        );\n        if (!response.data.hayError){\n          if (response.data.response.length > 0){\n          response.data.response.forEach(element => {            \n          this.items.push({\n            TipoGradoId: element[\"008TipoGradoId\"],\n            Nombre: element[\"008Nombre\"],\n            Activo: element[\"008Activo\"]\n          });\n        });\n          }\n        }else{\n        console.log(response);\n        this.$alert(\n          \"No se pudo obtener informaci√≥n, favor de volverlo a intentar.\"\n        );\n      }\n      this.isLoading = false;\n      } catch (err) {\n        console.log(err);\n      }\n    },\n    seleccionar: function(){                 \n        this.$emit(this.$props.funcion, this.valor);\n    }\n  }\n};\n</script>"],"sourceRoot":"src/components/Catalogos/Selects"}]}