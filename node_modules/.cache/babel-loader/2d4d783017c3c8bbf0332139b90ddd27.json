,oBAAA,CAAA,iBAAA,GAAA,KAAA;AACA,KAxCA;AAyCA,IAAA,UAAA,EAAA,oBAAA,IAAA,EAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,WAAA,YAAA,GAAA,CAAA,KAAA,YAAA,CAFA,CAIA;;AACA,WAAA,SAAA,CAAA,YAAA,CAAA,GAAA,GAAA,CAAA;AACA,KA/CA;AAgDA,IAAA,cAhDA,0BAgDA,KAhDA,EAgDA;AACA,WAAA,IAAA,CAAA,KAAA,EAAA,OAAA,GAAA,IAAA;AACA,KAlDA;AAmDA,IAAA,uBAnDA,mCAmDA,IAnDA,EAmDA;AACA,WAAA,SAAA,CAAA,YAAA,CAAA,GAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,UAAA,CAAA,kBAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,UAAA,CAAA,GAAA,GAAA,IAAA;AACA,KAvDA;AAwDA,IAAA,wBAxDA,oCAwDA,IAxDA,EAwDA;AACA,WAAA,YAAA,CAAA,WAAA,CAAA,GAAA,GAAA,IAAA;AACA,WAAA,YAAA,CAAA,KAAA,CAAA,GAAA,GAAA,IAAA,CAAA,KAAA;AAEA,UAAA,IAAA,CAAA,KAAA,CAAA,MAAA,GAAA,CAAA,EACA,KAAA,oBAAA,CAAA,UAAA,GAAA,IAAA,CADA,KAGA,KAAA,oBAAA,CAAA,UAAA,GAAA,KAAA;AACA,KAhEA;AAiEA,IAAA,qBAjEA,iCAiEA,IAjEA,EAiEA;AACA,WAAA,SAAA,CAAA,UAAA,CAAA,GAAA,CAAA,KAAA,GAAA,IAAA,CAAA,KAAA;AACA,WAAA,SAAA,CAAA,QAAA,CAAA,GAAA,GAAA,MAAA,CAAA,IAAA,CAAA,KAAA,CAAA,KAAA,CAAA,GAAA,EAAA,CAAA,CAAA,CAAA;AACA;AApEA;AA5HA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container is-max-desktop\">\r\n    <div class=\"col-12\" id=\"Titulo\">\r\n      <h1 class=\"titulo_azul\">Control de pagos</h1>\r\n      <div class=\"line_red\"></div>\r\n    </div>\r\n    <section id=\"filtro\">\r\n      <div class=\"col-12 row filtros_div\" id=\"filtros_div\">\r\n        <div class=\"col-12 filtro_titulo\">\r\n          <div class=\"row\">\r\n            <div class=\"col-10\">\r\n              <h1 class=\"col-10\">Favor de indicar filtros</h1>\r\n            </div>\r\n            <div\r\n              class=\"col-2\"\r\n              @click=\"mostrarFiltros = !mostrarFiltros\"\r\n              :key=\"mostrarFiltros\"\r\n            >\r\n              <i\r\n                class=\"fas fa-angle-down\"\r\n                v-if=\"!mostrarFiltros\"\r\n                style=\"float: right;\"\r\n              ></i>\r\n              <i\r\n                class=\"fas fa-angle-up\"\r\n                v-if=\"mostrarFiltros\"\r\n                style=\"float: right;\"\r\n              ></i>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div v-if=\"mostrarFiltros\" class=\"col-12 row\">\r\n          <div class=\"col-4\">\r\n            <label>Nombre</label>\r\n            <input\r\n              class=\"form-control\"\r\n              type=\"search\"\r\n              v-model=\"filtros.filtro_nombre\"\r\n            />\r\n          </div>\r\n          <div class=\"col-4\">\r\n            <label>Apellido Paterno</label>\r\n            <input\r\n              class=\"form-control\"\r\n              type=\"search\"\r\n              v-model=\"filtros.filtro_apellidoPaterno\"\r\n            />\r\n          </div>\r\n          <div class=\"col-4\">\r\n            <label>Apellido Materno</label>\r\n            <input\r\n              class=\"form-control\"\r\n              type=\"search\"\r\n              v-model=\"filtros.filtro_apellidoMaterno\"\r\n            />\r\n          </div>\r\n          <div class=\"col-4\">\r\n            <label>CURP</label>\r\n            <input\r\n              class=\"form-control\"\r\n              type=\"search\"\r\n              v-model=\"filtros.filtro_curp\"\r\n            />\r\n          </div>\r\n          <div class=\"col-4\">\r\n            <label>Número de control</label>\r\n            <input\r\n              class=\"form-control\"\r\n              type=\"number\"\r\n              v-model=\"filtros.filtro_numeroDeControl\"\r\n            />\r\n          </div>\r\n          <div class=\"col-4\">\r\n            <label class=\"activo_label\">Activo</label>\r\n            <select class=\"form-control\" v-model=\"filtros.filtro_activo\">\r\n              <option value=\"-1\">Seleccionar Activo</option>\r\n              <option value=\"1\">Si</option>\r\n              <option value=\"0\">No</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n        <div v-if=\"mostrarFiltros\" class=\"filtro_footer\">\r\n          <button\r\n            class=\"button is-default btn-sm mr-1\"\r\n            @click=\"limpiarFiltros()\"\r\n          >\r\n            Limpiar\r\n          </button>\r\n          <button class=\"button is-primary btn-sm\" @click=\"getAlumnos()\">\r\n            Buscar\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </section>\r\n    <br />\r\n    <section id=\"data_alumnos\">\r\n      <div class=\"row col-12 seccion_data\">\r\n        <div class=\"col-12 seccion_titulo_first\">\r\n          <h3>Datos del alumno</h3>\r\n        </div>\r\n        <div class=\"col-4 form-group padding-model\">\r\n          <label>Nombre</label>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            v-model=\"alumno_item.Nombre\"\r\n            :disabled=\"true\"\r\n          />\r\n        </div>\r\n        <div class=\"col-4 form-group padding-model\">\r\n          <label>Apellido Paterno</label>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            v-model=\"alumno_item.ApellidoPaterno\"\r\n            :disabled=\"true\"\r\n          />          \r\n        </div>\r\n        <div class=\"col-4 form-group padding-model\">\r\n          <label>Apellido Materno</label>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            v-model=\"alumno_item.ApellidoMaterno\"\r\n            :disabled=\"true\"\r\n          />\r\n        </div>\r\n        <div class=\"col-4 form-group padding-model\">\r\n          <label>CURP</label>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            v-model=\"alumno_item.Curp\"\r\n            :disabled=\"true\"\r\n          />\r\n        </div>        \r\n        <div class=\"col-4 form-group padding-model\">\r\n          <label>Número de control</label>\r\n          <input\r\n            type=\"number\"\r\n            class=\"form-control\"\r\n            v-model=\"alumno_item.NumeroDeControl\"\r\n            :disabled=\"true\"\r\n          />          \r\n        </div>\r\n        <div class=\"col-4 form-group padding-model\">\r\n          <label>Grupo</label>\r\n          <input\r\n            type=\"number\"\r\n            class=\"form-control\"\r\n            v-model=\"alumno_item.Grupo\"\r\n            :disabled=\"true\"\r\n          />          \r\n        </div>\r\n        <div class=\"col-12 seccion_titulo\">\r\n          <h1>Historial de pagos</h1>\r\n        </div>\r\n        <div id=\"table_pagos\">\r\n          <div class=\"col-12\" style=\"margin-bottom:100px;\">\r\n            <button\r\n              class=\"button is-primary mt-5 mb-1 align-left\"\r\n              @click=\"abrirModal('Agregar')\"\r\n            >\r\n              <i class=\"fas fa-plus\" style></i>&nbsp;&nbsp;Agregar pago\r\n            </button>\r\n            <br />\r\n            <!-- <div\r\n              class=\"row col-12\"\r\n              v-if=\"pagos_items.length <= 0\"\r\n              style=\"display: grid; justify-content: center;\"\r\n            >\r\n              <p>No se encontraron registros</p>\r\n            </div> -->\r\n            <div id=\"bootstrap_table\">\r\n              <div class=\"col-3 mr-0 align-rigth\">\r\n                <input\r\n                  class=\"form-control\"\r\n                  v-model=\"filter\"\r\n                  type=\"search\"\r\n                  placeholder=\"Buscar\"\r\n                />\r\n              </div>\r\n              <b-table\r\n                striped\r\n                hover\r\n                outlined\r\n                :items=\"pagos_items\"\r\n                :fields=\"fields\"\r\n                :per-page=\"perPage\"\r\n                :current-page=\"currentPage\"\r\n                :filter=\"filter\"\r\n              >\r\n                <template v-slot:cell(Activo)=\"data\">\r\n                  <button\r\n                    class=\"btn btn-default\"\r\n                    v-if=\"data.item.Activo == 1\"\r\n                    :key=\"data.item.AlumnoId\"\r\n                    style=\"cursor: default;\"\r\n                  >\r\n                    <i class=\"far fa-check-square\" style=\"color: green\"></i>\r\n                  </button>\r\n                  <button\r\n                    class=\"btn btn-default\"\r\n                    v-else\r\n                    :key=\"data.item.AlumnoId\"\r\n                    style=\"cursor: default;\"\r\n                  >\r\n                    <i class=\"far fa-times-circle\" style=\"color: red\"></i>\r\n                  </button>\r\n                </template>\r\n                <template v-slot:cell(opciones)=\"data\">\r\n                  <button\r\n                    class=\"button is-default is-small\"\r\n                    @click=\"abrirModal('')\"\r\n                    value=\"Detalles\"\r\n                    title=\"Ver Detalles\"\r\n                  >\r\n                    <i class=\"far fa-eye\"></i>\r\n                  </button>\r\n                  <button\r\n                    class=\"button is-info is-small\"\r\n                    @click=\"abrirModal('Editar')\"\r\n                    value=\"Editar\"\r\n                    title=\"Editar\"\r\n                  >\r\n                    <i class=\"far fa-edit\"></i>\r\n                  </button>\r\n                  <button\r\n                    class=\"button is-danger is-small\"\r\n                    :disabled=\"data.item.Activo == 0\"\r\n                    @click=\"cancelar(data.item)\"\r\n                    value=\"Eliminar\"\r\n                    title=\"Cancelar\"\r\n                  >\r\n                    <i class=\"fas fa-ban\"></i>\r\n                  </button>\r\n                </template>\r\n              </b-table>\r\n              <b-pagination\r\n                v-model=\"currentPage\"\r\n                :total-rows=\"rows\"\r\n                :per-page=\"perPage\"\r\n              ></b-pagination>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n    <section id=\"modal\">\r\n      <div v-if=\"mostrarModal\" class=\"modal_div\" id=\"modal_div\">\r\n        <transition name=\"modal\">\r\n          <div class=\"modal-mask\">\r\n            <div class=\"modal-wrapper\">\r\n              <div class=\"modal-dialog modal-xl\">\r\n                <div class=\"modal-content modal-xl\">\r\n                  <div class=\"modal-header\">\r\n                    <div class=\"col-11\">\r\n                      <h2 class=\"modal-title text-center\">\r\n                        {{ titutoModal }} tipo de pago\r\n                      </h2>\r\n                      <div class=\"line_red\"></div>\r\n                    </div>\r\n                    <a class=\"button close_modal\" @click=\"mostrarModal = false\">\r\n                      <span aria-hidden=\"true\">&times;</span>\r\n                    </a>\r\n                  </div>\r\n                  <div class=\"modal-body\">\r\n                    <div class=\"row\">\r\n                      <section id=\"data_alumnos\">\r\n                        <div class=\"row seccion_modal\">\r\n                          <div class=\"col-4 form-group padding-model\">       \r\n                            <tipos-pago\r\n                                :key=\"pago_item.TipoDePago.key\"\r\n                                :titulo=\"true\"\r\n                                :label=\"'Tipo de pago*'\"\r\n                                :TipoDeCicloEscolarId= \"pago_item.TipoDePago.tipoDeCicloEscolar > 0 ? pago_item.TipoDePago.tipoDeCicloEscolar : null\"\r\n                                v-on:seleccionarTipoDePago=\"seleccionarTipoDePago($event)\"\r\n                               :funcion=\"'seleccionarTipoDePago'\"\r\n                              />\r\n                            <p v-if=\"!pago_item.TipoDePago.isValid\" style=\"color: red\">\r\n                              Favor de ingresar un tipo de pago\r\n                              <i\r\n                                class=\"far fa-times-circle\"\r\n                                style=\"color: red\"\r\n                              ></i>\r\n                            </p>\r\n                          </div>\r\n                          <div class=\"col-4 form-group padding-model\">\r\n                            <label>Cantidad*</label>\r\n                            <input\r\n                              type=\"text\"\r\n                              class=\"form-control\"\r\n                              v-model=\"pago_item.TipoDePago.val.Monto\"\r\n                              :disabled=\"!inhabilitar\"\r\n                              @blur=\"limpiarValidez('Cantidad')\"\r\n                            />\r\n                            <p v-if=\"!pago_item.Cantidad.isValid\" style=\"color: red\">\r\n                              Favor de ingresar monto\r\n                              <i\r\n                                class=\"far fa-times-circle\"\r\n                                style=\"color: red\"\r\n                              ></i>\r\n                            </p>\r\n                          </div>  \r\n                          <div class=\"col-4\">\r\n                            <TiposCicloEscolar\r\n                                :key=\"pago_item.CicloEscolar.val\"\r\n                                :titulo=\"true\"\r\n                                :label=\"'Ciclo Escolar*'\"\r\n                                :actual=\"true\"\r\n                                :tipoDeCicloEscolarId=\"pago_item ? pago_item.CicloEscolar.val : -1\"\r\n                                v-on:seleccionarCicloEscolar=\"seleccionarCicloEscolar($event)\"\r\n                               :funcion=\"'seleccionarCicloEscolar'\"\r\n                             />\r\n                          </div>                                                                                                                                                                                                                                                                                 \r\n                        </div>\r\n                      </section>                        \r\n                      <section>\r\n                        <div class=\"row seccion_modal\">      \r\n                          <div class=\"col-12 seccion_titulo_modal\">\r\n                            <h3>Recargos</h3>\r\n                          </div>\r\n                          <div class=\"col-4 form-group padding-model\">\r\n                            <tipos-recargos\r\n                              :key=\"recargo_item.TipoRecargo.val.TipoDeRecargoId\"\r\n                              :label=\"'Recargo'\" \r\n                              :titulo=\"true\"\r\n                              :tipoDeRecargoId=\"pago_item ? recargo_item.TipoRecargo.val.TipoDeRecargoId : -1\"\r\n                              @seleccionarTipoDeRecargo=\"seleccionarTipoDeRecargo($event)\"\r\n                              :funcion=\"'seleccionarTipoDeRecargo'\"\r\n                            ></tipos-recargos>\r\n                          </div>\r\n                          <div class=\"col-4 form-group padding-model\">\r\n                            <label>Monto*</label>\r\n                            <input\r\n                              type=\"text\"\r\n                              class=\"form-control\"\r\n                              v-model=\"recargo_item.Monto.val\"\r\n                              :key=\"recargo_item.Monto.val\"\r\n                              :disabled=\"mostrarEditarRecargo.desabledMonto\"                              \r\n                            />                            \r\n                          </div> \r\n                          <div class=\"col-4 form-group padding-model\">\r\n                            <label>Fecha límite*</label>\r\n                            <input\r\n                              type=\"date\"\r\n                              class=\"form-control\"\r\n                              v-model=\"recargo_item.Fecha.val\"\r\n                              :disabled=\"!inhabilitar\"\r\n                            />                            \r\n                          </div>  \r\n                          <div class=\"modal-footer\" style=\"border: 0px;\" v-if=\"mostrarEditarRecargo.displayBtn\">\r\n                            <button v-show=\"mostrarEditarRecargo.displayRecargoBtn\" type=\"button\" class=\"button\" @click=\"mostrarEditarRecargo.quienAutoriza = true\">Editar recargo</button>\r\n                            <button type=\"button\" class=\"button is-primary\" @click=\"agregarRecargo\">Agregar recargo</button>\r\n                          </div>\r\n                          <div class=\"col-12 form-group padding-model\" v-if=\"mostrarEditarRecargo.quienAutoriza\">\r\n                            <label>Quien autoriza*</label>\r\n                            <input\r\n                              type=\"text\"\r\n                              class=\"form-control\"\r\n                              v-model=\"recargo_item.Autoriza.val\"\r\n                              :disabled=\"!inhabilitar\"\r\n                              @blur=\"autorizaEvt\"\r\n                            />                            \r\n                          </div>\r\n                          <section id=\"data_table\">\r\n                            <br/>\r\n                          <div class=\"col-12 form-group padding-model\">\r\n                            <table class=\"table\">\r\n                              <thead>\r\n                                <tr>\r\n                                  <th>Concepto</th>\r\n                                  <th>Cantidad</th>\r\n                                  <th>Días de retraso</th>\r\n                                  <th>Total a pagar</th>\r\n                                  <th></th>\r\n                                </tr>\r\n                              </thead>\r\n                              <tbody>\r\n                                <tr\r\n                                  v-for=\"item in itemsRecargo\"\r\n                                  :key=\"item.Id\"\r\n                                >\r\n                                  <td>{{ item.TipoRecargo }}</td>\r\n                                  <td>{{ item.Monto }}</td>\r\n                                  <td>{{ item.DiasRetraso }}</td>\r\n                                  <td>{{ item.TotalAPagar }}</td>\r\n                                  <td>\r\n                                    <button\r\n                                      class=\"btn btn-default\"\r\n                                      @click=\"eliminarRecargo(item)\"                                          \r\n                                    >\r\n                                      <i class=\"fas fa-trash-alt\"></i>\r\n                                    </button>\r\n                                  </td>\r\n                                </tr>\r\n                              </tbody>\r\n                            </table>\r\n                          </div> \r\n                          </section>\r\n                          <section id=\"data_total\">\r\n                            <br />                                \r\n                            <div class=\"col-12 seccion_total_modal\">\r\n                              <label>Total</label>\r\n                              <label style=\"float: right;\">${{ pago_item.Cantidad.val }}</label>\r\n                            </div>                            \r\n                          </section>\r\n                        </div>\r\n                      </section>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"modal-footer\">\r\n                    <button\r\n                      type=\"button\"\r\n                      class=\"button is-primary\"\r\n                      v-if=\"inhabilitar\"\r\n                      @click=\"guardarAlumno()\"\r\n                    >\r\n                      Guardar\r\n                    </button>\r\n                    <button\r\n                      type=\"button\"\r\n                      class=\"btn btn-secondary\"\r\n                      data-dismiss=\"modal\"\r\n                      @click=\"mostrarModal = false\"\r\n                      v-if=\"inhabilitar\"\r\n                    >\r\n                      Cancelar\r\n                    </button>\r\n                    <button\r\n                      type=\"button\"\r\n                      class=\"btn btn-secondary\"\r\n                      data-dismiss=\"modal\"\r\n                      @click=\"mostrarModal = false\"\r\n                      v-if=\"!inhabilitar\"\r\n                    >\r\n                      Cerrar\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </transition>\r\n      </div>\r\n    </section>\r\n    <cargando v-if=\"isLoading\"></cargando>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TiposCicloEscolar from \"../Catalogos/Selects/TiposCicloEscolar.vue\";\r\nimport TiposPago from \"../Catalogos/Selects/TiposPagos.vue\";\r\nimport TiposRecargos from \"../../components/Catalogos/Selects/TiposRecargos.vue\";\r\nimport moment from 'moment'\r\n\r\nexport default {\r\n  components:{\r\n    TiposCicloEscolar,\r\n    TiposPago,\r\n    TiposRecargos\r\n  },\r\n  data() {\r\n    return {\r\n      isLoading: false,\r\n      mostrarFiltros: true,\r\n      mostrarModal: false,\r\n      mostrarEditarRecargo: {\r\n        displayRecargoBtn: true,\r\n        displayBtn: false,\r\n        quienAutoriza: false,\r\n        desabledMonto: true\r\n      },\r\n      titutoModal: \"\",\r\n      filtros: {\r\n        filtro_nombre: \"\",\r\n        filtro_apellidoPaterno: \"\",\r\n        filtro_apellidoMaterno: \"\",\r\n        filtro_curp: \"\",\r\n        filtro_numeroDeControl: \"\",\r\n        filtro_activo: \"-1\",\r\n      },\r\n      inhabilitar: true,\r\n      perPage: 5,\r\n      currentPage: 1,\r\n      pagos_items: [],\r\n      filter: \"\",\r\n      itemsRecargo:[],\r\n      fields: [\r\n        {\r\n          key: \"AlumnoId.val\",\r\n          label: \"Folio\",\r\n          sortable: true,\r\n        },\r\n        {\r\n          key: \"NombreCompleto.val\",\r\n          label: \"Tipo de pago\",\r\n          sortable: true,\r\n        },\r\n        {\r\n          key: \"Curp.val\",\r\n          label: \"Cantidad\",\r\n          sortable: true,\r\n        },\r\n        {\r\n          key: \"NumeroDeControl.val\",\r\n          label: \"Fecha de pago\",\r\n          sortable: true,\r\n        },\r\n        {\r\n          label: \"Opciones\",\r\n          key: \"opciones\",\r\n        },\r\n      ],\r\n      alumno_item: {\r\n        AlumnoId: '',\r\n        Nombre: '',\r\n        ApellidoPaterno: '',\r\n        ApellidoMaterno: '',\r\n        Curp: '',\r\n        Genero: '',\r\n        NumeroDeControl: 0,\r\n        Grupo: ''\r\n      },\r\n      pago_item: {\r\n        CicloEscolar:{\r\n          val: null,\r\n          isValid: true,\r\n        },\r\n        PagoId: {\r\n            val: null,\r\n            isValid: true,\r\n        },\r\n        TipoDePago:\r\n        {\r\n            key: '',\r\n            val: {\r\n              Monto: 0\r\n            },\r\n            isValid: true,\r\n            tipoDeCicloEscolar: -1\r\n        },\r\n        Cantidad:\r\n        {\r\n            val: 0,\r\n            isValid: true,\r\n        },  \r\n        Recargos: []      \r\n      },\r\n      recargo_item: {\r\n          TipoRecargo:{\r\n            val: {\r\n              TipoDeRecargoId: 0              \r\n            },\r\n            isValid: true,\r\n          },\r\n          Monto: {\r\n            val: null,\r\n            isValid: true\r\n          },\r\n          Fecha: {\r\n            val: moment().format('yyyy-MM-DD'),\r\n            isValid: true\r\n          },\r\n          Autoriza:{\r\n            val: null,\r\n            isValid: true,\r\n          },\r\n          TotalAPagar: 0,\r\n          DiasRetraso: 0\r\n      },\r\n      indexRecargos: 0\r\n      \r\n    };\r\n  },\r\n  computed: {\r\n    rows() {\r\n      return this.pagos_items.length;\r\n    },\r\n  },\r\n  methods:{\r\n    eliminarRecargo(item) {\r\n      const index = this.itemsRecargo.indexOf(\r\n        this.itemsRecargo.find((i) => i.Id === item.Id)\r\n      );\r\n      this.itemsRecargo.splice(index, 1);      \r\n    },\r\n    async agregarRecargo(){\r\n      await this.calcularTotal();\r\n      this.mostrarEditarRecargo.quienAutoriza = true;      \r\n\r\n      this.itemsRecargo.push({\r\n        Id: this.indexRecargos,\r\n        TipoRecargo: this.recargo_item.TipoRecargo.val.Nombre,\r\n        Monto: this.recargo_item.TipoRecargo.val.Monto,\r\n        DiasRetraso: this.recargo_item.DiasRetraso,\r\n        TotalAPagar: this.recargo_item.TotalAPagar\r\n      });\r\n      this.indexRecargos ++;\r\n      this.limpiarRecargo();\r\n    },\r\n    async calcularTotal (){\r\n      var diasRetraso = Math.trunc((new Date().getTime() - new Date(this.recargo_item.Fecha.val).getTime()) / (1000 * 60 * 60 * 24)) - 1;\r\n      this.recargo_item.DiasRetraso = diasRetraso;\r\n      this.recargo_item.TotalAPagar = diasRetraso * Number(this.recargo_item.Monto.val.split('$')[1]);\r\n      this.pago_item.Cantidad.val += this.recargo_item.TotalAPagar;\r\n    },\r\n    limpiarRecargo(){\r\n      this.recargo_item.TipoRecargo.val = { TipoDeRecargoId: 0 };\r\n      this.recargo_item.Monto.val = null;\r\n      this.recargo_item.Fecha.val = moment().format('yyyy-MM-DD');\r\n      this.recargo_item.Autoriza.val = null;\r\n      this.recargo_item.TotalAPagar = 0;\r\n      this.recargo_item.DiasRetraso = 0;\r\n      this.mostrarEditarRecargo.quienAutoriza = false;\r\n      this.mostrarEditarRecargo.desabledMonto = true;\r\n    },\r\n    autorizaEvt(){\r\n      this.mostrarEditarRecargo.desabledMonto = false;\r\n      this.mostrarEditarRecargo.displayRecargoBtn = false;\r\n    },\r\n    abrirModal: function(tipo) {     \r\n      this.titutoModal = tipo;            \r\n      this.mostrarModal = !this.mostrarModal;\r\n\r\n      //Cargar ciclo escolar\r\n      this.pago_item.CicloEscolar.val = 1; \r\n    },\r\n    limpiarValidez(input) {\r\n      this.item[input].isValid = true;\r\n    },\r\n    seleccionarCicloEscolar(item){\r\n      this.pago_item.CicloEscolar.val = item;\r\n      this.pago_item.TipoDePago.tipoDeCicloEscolar = item;\r\n      this.pago_item.TipoDePago.key = item;\r\n    },\r\n    seleccionarTipoDeRecargo(item){        \r\n      this.recargo_item.TipoRecargo.val = item;\r\n      this.recargo_item.Monto.val = item.Monto;   \r\n      \r\n      if(item.Monto.length > 2)\r\n        this.mostrarEditarRecargo.displayBtn = true; \r\n      else\r\n        this.mostrarEditarRecargo.displayBtn = false; \r\n    },\r\n    seleccionarTipoDePago(item){      \r\n      this.pago_item.TipoDePago.val.Monto = item.monto;\r\n      this.pago_item.Cantidad.val = Number(item.monto.split('$')[1]);\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n.seccion_data {\r\n  border: 1px solid #adb5bd;\r\n  padding-bottom: 10px;\r\n  border-radius: 5px;\r\n  margin-left: 0px;\r\n}\r\n\r\n.seccion_titulo_first {\r\n  background-color: #dbdbdb;\r\n  padding-top: 10px;\r\n  padding-bottom: 10px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.seccion_titulo {\r\n  width: 100%;\r\n  padding: 10px;\r\n  border: 1px solid #ccc;\r\n  margin: 20px 0 0px 0;\r\n  background-color: #dbdbdb;\r\n}\r\n\r\n.seccion_modal {\r\n  padding: 10px;\r\n  margin: 5px;\r\n  width: 99%;\r\n  border: 1px solid #ccc;\r\n  border-radius: 10px;\r\n}\r\n\r\n.seccion_titulo_modal {\r\n  width: 100%;\r\n  padding: 10px;\r\n  border-bottom: 1px solid #ccc;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.seccion_total_modal {\r\n  width: 100%;\r\n  padding: 10px;\r\n  border-top: 1px solid #ccc;\r\n  margin-top: 5px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/Administracion"}]}