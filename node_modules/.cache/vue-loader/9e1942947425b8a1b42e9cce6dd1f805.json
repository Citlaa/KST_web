AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"RegistroDeAlumnos.vue","sourceRoot":"src/components/Alumnos","sourcesContent":["<template>\r\n  <div class=\"container is-max-desktop\">\r\n    <div class=\"col-12\" id=\"Titulo\">\r\n      <h1 class=\"titulo_azul\">Registro de Alumnos</h1>\r\n      <div class=\"line_red\"></div>\r\n    </div>\r\n    <div class=\"col-12 row filtros_div\" id=\"filtros_div\">\r\n      <div class=\"col-12 filtro_titulo\">\r\n        <div class=\"row\">\r\n          <div class=\"col-10\">\r\n            <h1 class=\"col-10\">Favor de indicar filtros</h1>\r\n          </div>\r\n          <div\r\n            class=\"col-2\"\r\n            @click=\"mostrarFiltros = !mostrarFiltros\"\r\n            :key=\"mostrarFiltros\"\r\n          >\r\n            <i\r\n              class=\"fas fa-angle-down\"\r\n              v-if=\"!mostrarFiltros\"\r\n              style=\"float: right;\"\r\n            ></i>\r\n            <i\r\n              class=\"fas fa-angle-up\"\r\n              v-if=\"mostrarFiltros\"\r\n              style=\"float: right;\"\r\n            ></i>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div v-if=\"mostrarFiltros\" class=\"col-12 row\">\r\n        <div class=\"col-4\">\r\n          <label>Nombre</label>\r\n          <input\r\n            class=\"form-control\"\r\n            type=\"text\"\r\n            v-model=\"filtros.filtro_nombre\"\r\n          />\r\n        </div>\r\n        <div class=\"col-4\">\r\n          <label>Apellido Paterno</label>\r\n          <input\r\n            class=\"form-control\"\r\n            type=\"text\"\r\n            v-model=\"filtros.filtro_apellidoPaterno\"\r\n          />\r\n        </div>\r\n        <div class=\"col-4\">\r\n          <label>Apellido Materno</label>\r\n          <input\r\n            class=\"form-control\"\r\n            type=\"text\"\r\n            v-model=\"filtros.filtro_apellidoMaterno\"\r\n          />\r\n        </div>\r\n        <div class=\"col-4\">\r\n          <label>CURP</label>\r\n          <input\r\n            class=\"form-control\"\r\n            type=\"text\"\r\n            v-model=\"filtros.filtro_curp\"\r\n          />\r\n        </div>\r\n        <div class=\"col-4\">\r\n          <label>Número de control</label>\r\n          <input\r\n            class=\"form-control\"\r\n            type=\"number\"\r\n            v-model=\"filtros.filtro_numeroDeControl\"\r\n          />\r\n        </div>\r\n        <div class=\"col-4\">\r\n          <label class=\"activo_label\">Activo</label>\r\n          <select class=\"form-control\" v-model=\"filtros.filtro_activo\">\r\n            <option value=\"-1\">Seleccionar Activo</option>\r\n            <option value=\"1\">Si</option>\r\n            <option value=\"0\">No</option>\r\n          </select>\r\n        </div>\r\n      </div>\r\n      <div v-if=\"mostrarFiltros\" class=\"filtro_footer\">\r\n        <button class=\"button is-default btn-sm mr-1\" @click=\"limpiarFiltros()\">\r\n          Limpiar\r\n        </button>\r\n        <button class=\"button is-primary btn-sm\" @click=\"getAlumnos()\">\r\n          Buscar\r\n        </button>\r\n      </div>\r\n    </div>\r\n    <div class=\"col-12\" style=\"margin-bottom:100px;\">\r\n      <button\r\n        class=\"button is-primary mt-5 mb-1 align-left\"\r\n        @click=\"abrirModal('Agregar', {})\"\r\n      >\r\n        <i class=\"fas fa-plus\" style></i>&nbsp;&nbsp;Agregar alumno\r\n      </button>\r\n      <br />\r\n      <div id=\"bootstrap_table\">\r\n        <div class=\"col-3 mr-0 align-rigth\">\r\n          <input\r\n            class=\"form-control\"\r\n            v-model=\"filter\"\r\n            type=\"search\"\r\n            placeholder=\"Buscar\"\r\n          />\r\n        </div>\r\n        <b-table\r\n          striped\r\n          hover\r\n          outlined\r\n          :items=\"items\"\r\n          :fields=\"fields\"\r\n          :per-page=\"perPage\"\r\n          :current-page=\"currentPage\"\r\n          :filter=\"filter\"\r\n        >\r\n          <template v-slot:cell(Activo)=\"data\">\r\n            <i\r\n              v-if=\"data.item.Activo == 1\"\r\n              class=\"far fa-check-square\"\r\n              style=\"color: green\"\r\n            ></i>\r\n            <i v-else class=\"far fa-times-circle\" style=\"color: red\"></i>\r\n          </template>\r\n          <template v-slot:cell(opciones)=\"data\">\r\n            <button\r\n              class=\"button is-info is-small\"\r\n              @click=\"abrirModal('Editar', data.item)\"\r\n              value=\"Editar\"\r\n              title=\"Editar\"\r\n            >\r\n              <i class=\"far fa-edit\"></i>\r\n            </button>\r\n            <button\r\n              class=\"button is-danger is-small\"\r\n              :disabled=\"data.item.Activo == 0\"\r\n              @click=\"cancelar(data.item)\"\r\n              value=\"Eliminar\"\r\n              title=\"Cancelar\"\r\n            >\r\n              <i class=\"fas fa-ban\"></i>\r\n            </button>\r\n          </template>\r\n        </b-table>\r\n        <b-pagination\r\n          v-model=\"currentPage\"\r\n          :total-rows=\"rows\"\r\n          :per-page=\"perPage\"\r\n        ></b-pagination>\r\n      </div>\r\n    </div>\r\n    <div v-if=\"mostrarModal\" class=\"modal_div\" id=\"modal_div\">\r\n      <transition name=\"modal\">\r\n        <div class=\"modal-mask\">\r\n          <div class=\"modal-wrapper\">\r\n            <div class=\"modal-dialog modal-xl\">\r\n              <div class=\"modal-content modal-xl\">\r\n                <div class=\"modal-header\">\r\n                  <div class=\"col-11\">\r\n                    <h2 class=\"modal-title text-center\">\r\n                      {{ titutoModal }} Alumno\r\n                    </h2>\r\n                    <div class=\"line_red\"></div>\r\n                  </div>\r\n                  <a class=\"button close_modal\" @click=\"mostrarModal = false\">\r\n                    <span aria-hidden=\"true\">&times;</span>\r\n                  </a>\r\n                </div>\r\n                <div class=\"modal-body\">\r\n                  <div class=\"row\">\r\n                    <div class=\"row seccion_modal\">\r\n                      <div class=\"col-12 seccion_titulo_modal\">\r\n                        <h3>Datos del alumno</h3>\r\n                      </div>\r\n                      <div class=\"col-4 form-group padding-model\">\r\n                        <label>Nombre</label>\r\n                        <input\r\n                          type=\"number\"\r\n                          class=\"form-control\"\r\n                          v-model=\"item.Nombre\"\r\n                        />\r\n                      </div>\r\n                      <div class=\"col-4 form-group padding-model\">\r\n                        <label>Apellido Paterno</label>\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          v-model=\"item.ApellidoPaterno\"\r\n                        />\r\n                      </div>\r\n                      <div class=\"col-4 form-group padding-model\">\r\n                        <label>Apellido Materno</label>\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          v-model=\"item.ApellidoMaterno\"\r\n                        />\r\n                      </div>\r\n                      <div class=\"col-4 form-group padding-model\">\r\n                        <label>CURP</label>\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          v-model=\"item.Curp\"\r\n                        />\r\n                      </div>\r\n                      <div class=\"col-4 form-group padding-model\">\r\n                        <label>Fecha de nacimiento</label>\r\n                        <input\r\n                          type=\"date\"\r\n                          class=\"form-control\"\r\n                          v-model=\"item.FechaDeNacimiento\"\r\n                        />\r\n                      </div>\r\n                      <div class=\"col-4 padding-model\">\r\n                        <label>Genero</label>\r\n                        <select class=\"form-control\" v-model=\"item.GeneroId\">\r\n                          <option value=\"Femenino\">Femenino</option>\r\n                          <option value=\"Masculino\">Masculino</option>\r\n                        </select>\r\n                      </div>\r\n                      <div class=\"col-4 form-group padding-model\">\r\n                        <label>Número de control</label>\r\n                        <input\r\n                          type=\"number\"\r\n                          class=\"form-control\"\r\n                          v-model=\"item.NumeroDeControl\"\r\n                        />\r\n                      </div>\r\n                      <div class=\"col-4 form-group padding-model\">\r\n                        <label>Escuela de procedencia</label>\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          v-model=\"item.Escuela\"\r\n                        />\r\n                      </div>\r\n                      <div class=\"col-4 form-group padding-model\">\r\n                        <label>Promedio de procedencia</label>\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          v-model=\"item.Promedio\"\r\n                        />\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"row seccion_modal\">\r\n                      <div class=\"col-12 seccion_titulo_modal\">\r\n                        <h3>Padre o Tutor</h3>\r\n                        <p class=\"mt-1\" style=\"color: red\">\r\n                          Favor de buscar el nombre del padre o tutor, si no\r\n                          existe agregarlo.\r\n                        </p>\r\n                      </div>\r\n                      <div class=\"col-4 form-group padding-model\">\r\n                        <label>Nombre</label>\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          v-model=\"item.Promedio\"\r\n                        />\r\n                      </div>\r\n                      <div class=\"col-4 form-group padding-model\">\r\n                        <label>Apellido Paterno</label>\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          v-model=\"item.Promedio\"\r\n                        />\r\n                      </div>\r\n                      <div class=\"col-4 form-group padding-model\">\r\n                        <label>Apellido Materno</label>\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          v-model=\"item.Promedio\"\r\n                        />\r\n                      </div>\r\n                      <div\r\n                        class=\"mt-2\"\r\n                        style=\"display:flex; justify-content: flex-end;\"\r\n                      >\r\n                        <button\r\n                          type=\"button\"\r\n                          class=\"button is-primary\"\r\n                          @click=\"guardarTipoDeCicloEscolar()\"\r\n                        >\r\n                          Buscar\r\n                        </button>\r\n                      </div>\r\n                      <div class=\"col-12\">\r\n                        <div\r\n                          class=\"row\"\r\n                          v-if=\"padres\"\r\n                          style=\"display: grid; justify-content: center;\"\r\n                        >\r\n                          <div class=\"col-12\"><p>No hay registros</p></div>\r\n                          <div class=\"col-12\">\r\n                            <button class=\"button is-default\">\r\n                              Agregar Padre/Tutor\r\n                            </button>\r\n                          </div>\r\n                        </div>\r\n                        <table v-else class=\"table\">\r\n                          <thead>\r\n                            <tr>\r\n                              <th>\r\n                                Folio\r\n                              </th>\r\n                              <th>\r\n                                Nombre\r\n                              </th>\r\n                              <th>\r\n                                Apellido Paterno\r\n                              </th>\r\n                              <th>\r\n                                Apellido Materno\r\n                              </th>\r\n                              <th>\r\n                                Telefono\r\n                              </th>\r\n                              <th>\r\n                                Domicilio\r\n                              </th>\r\n                              <th></th>\r\n                            </tr>\r\n                          </thead>\r\n                          <tbody>\r\n                            <tr>\r\n                              <th></th>\r\n                              <th></th>\r\n                              <th></th>\r\n                              <th></th>\r\n                              <th></th>\r\n                              <th></th>\r\n                              <th>\r\n                                <input type=\"radio\" />\r\n                              </th>\r\n                            </tr>\r\n                          </tbody>\r\n                        </table>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"modal-footer\">\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"button is-primary\"\r\n                    @click=\"guardarTipoDeCicloEscolar()\"\r\n                  >\r\n                    Guardar\r\n                  </button>\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"btn btn-secondary\"\r\n                    data-dismiss=\"modal\"\r\n                    @click=\"mostrarModal = false\"\r\n                  >\r\n                    Cancelar\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </transition>\r\n    </div>\r\n    <cargando v-if=\"isLoading\"></cargando>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport routeAPI from \"@/js/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      isLoading: false,\r\n      mostrarFiltros: true,\r\n      mostrarModal: false,\r\n      filtros: {\r\n        filtro_nombre: \"\",\r\n        filtro_apellidoPaterno: \"\",\r\n        filtro_apellidoMaterno: \"\",\r\n        filtro_curp: \"\",\r\n        filtro_numeroDeControl: \"\",\r\n        filtro_activo: \"-1\",\r\n      },\r\n      perPage: 5,\r\n      currentPage: 1,\r\n      fields: [\r\n        {\r\n          key: \"AlumnoId\",\r\n          label: \"Folio\",\r\n          sortable: true,\r\n        },\r\n        {\r\n          key: \"Nombre\",\r\n          label: \"Nombre\",\r\n          sortable: true,\r\n        },\r\n        {\r\n          key: \"Curp\",\r\n          label: \"CURP\",\r\n          sortable: true,\r\n        },\r\n        {\r\n          key: \"NumeroDeControl\",\r\n          label: \"Número de Control\",\r\n          sortable: true,\r\n        },\r\n        {\r\n          key: \"Genero\",\r\n          label: \"Genero\",\r\n        },\r\n        {\r\n          key: \"TipoEstadoAlumnoId\",\r\n          label: \"Estado\",\r\n        },\r\n      ],\r\n      filter: \"\",\r\n      item: {\r\n        Nombre: \"\",\r\n        ApellidoPaterno: \"\",\r\n        ApellidoMaterno: \"\",\r\n        Curp: \"\",\r\n        FechaDeNacimiento: \"\",\r\n        GeneroId: 0,\r\n        NumeroDeControl: 0,\r\n        Escuela: \"\",\r\n        Promedio: 0,\r\n      },\r\n      items: [],\r\n      titutoModal: \"\",\r\n      padres: [],\r\n    };\r\n  },\r\n  computed: {\r\n    rows() {\r\n      return this.items.length;\r\n    },\r\n  },\r\n  created() {\r\n    this.getEstadosAlumno();\r\n    this.getAlumnos();\r\n  },\r\n  methods: {\r\n    abrirModal: function() {\r\n      this.mostrarModal = !this.mostrarModal;\r\n    },\r\n    async getAlumnos() {\r\n      try {\r\n        this.isLoading = true;\r\n        this.items = [];\r\n        const filtros = {\r\n          filtro: {},\r\n        };\r\n        console.log(this.filtros);\r\n        console.log(\"this.filtros.filtro_curp\");\r\n        if (this.filtros.filtro_nombre != \"\")\r\n          filtros.filtro.nombre = this.filtros.filtro_nombre;\r\n        if (this.filtros.filtro_apellidoPaterno != \"\")\r\n          filtros.filtro.apellidoPaterno = this.filtros.filtro_apellidoPaterno;\r\n        if (this.filtros.filtro_apellidoMaterno != \"\")\r\n          filtros.filtro.apellidoMaterno = this.filtros.filtro_apellidoMaterno;\r\n        if (this.filtros.filtro_curp != \"\")\r\n          filtros.filtro.curp = this.filtros.filtro_curp;\r\n        if (this.filtros.filtro_numeroDeControl != \"\")\r\n          filtros.filtro.numeroDeControl = Number(\r\n            this.filtros.filtro_numeroDeControl\r\n          );\r\n        if (this.filtros.filtro_activo != \"\")\r\n          filtros.filtro.activo = Number(this.filtros.filtro_activo);\r\n\r\n        const response = await axios.post(\r\n          routeAPI + \"alumnos/alumnos\",\r\n          filtros\r\n        );\r\n        console.log(response);\r\n        if (!response.data.hayError) {\r\n          response.data.response.forEach((element) => {\r\n            this.items.push({\r\n              AlumnoId: element[\"011AlumnoId\"],\r\n              Nombre:\r\n                element[\"011Nombre\"] +\r\n                \" \" +\r\n                element[\"011ApellidoPaterno\"] +\r\n                \" \" +\r\n                element[\"011ApellidoMaterno\"],\r\n              Curp: element[\"011CURP\"],\r\n              FechaNacimiento: element[\"011FechaNacimiento\"],\r\n              Genero: element[\"011Genero\"],\r\n              NumeroDeControl: element[\"011NumeroDeControl\"],\r\n              EscuelaDeProcedenciaId: element[\"015EscuelaDeProcedenciaId\"],\r\n              PromedioDeProcedencia: element[\"011PromedioDeProcedencia\"],\r\n              Domicilio: element[\"011Domicilio\"],\r\n              TipoEstadoAlumnoId: element[\"014TipoEstadoAlumnoId\"],\r\n              Activo: element[\"011Activo\"],\r\n            });\r\n          });\r\n        } else {\r\n          console.log(response);\r\n          this.$alert(\r\n            \"No se pudo obtenera información, favor de volverlo a intentar.\"\r\n          );\r\n        }\r\n        this.isLoading = false;\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n    async getEstadosAlumno(){\r\n      try {\r\n        this.isLoading = true;\r\n        const filtros = {\r\n          filtro: {\r\n            activo: 1,\r\n          },\r\n        };\r\n\r\n        const response = await axios.post(\r\n          routeAPI + \"catalogo/tiposDeModalidad\",\r\n          filtros\r\n        );\r\n\r\n        response.data.response.forEach((element) => {\r\n          this.modalidades.push({\r\n            TipoDeModalidadId: element[\"004TipoModalidadId\"],\r\n            Nombre: element[\"004Nombre\"],\r\n            Activo: element[\"004Activo\"],\r\n          });\r\n        });\r\n        this.isLoading = false;\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.modal-content {\r\n  width: 1140px;\r\n}\r\n\r\n.seccion_modal {\r\n  padding: 10px;\r\n  margin: 5px;\r\n  width: 99%;\r\n  border: 1px solid #ccc;\r\n  border-radius: 10px;\r\n}\r\n\r\n.seccion_titulo_modal {\r\n  /* display: flex; */\r\n  width: 100%;\r\n  padding: 10px;\r\n  border-bottom: 1px solid #ccc;\r\n  margin-bottom: 5px;\r\n}\r\n</style>\r\n"]}]}